{
  "name": "dodiddone",
  "version": "1.0.0",
  "description": "Application to track work.",
  "main": "index.js",
  "scripts": {
    "postinstall": "webpack --config ./webpack-production.config.js --progress --colors",
    "start": "node server.js",
    "lint": "eslint . --ext .js --ext .jsx",
    "heroku-off": "heroku ps:scale web=0 worker=0 builder=0",
    "heroku-on": "heroku ps:scale web=1 builder=1 worker=1",
    "heroku-push": "localbranch=$(git rev-parse --abbrev-ref HEAD) && git push --force heroku $localbranch:master",
    "heroku-temp-push": "git commit -am 'temp for debugging' && npm run heroku-push && git reset --soft 'HEAD^' 2>&1",
    "heroku-latest": "git ls-remote heroku | awk 'END{print $1}' | xargs git show",
    "hlogs": "heroku logs -t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trussworks/DoDidDone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trussworks/DoDidDone/issues"
  },
  "homepage": "https://github.com/trussworks/DoDidDone#readme",
  "dependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.15.0",
    "material-ui": "^0.14.4",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.4",
    "react-dom": "^0.14.8",
    "react-tap-event-plugin": "^0.2.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "eslint": "^2.5.3",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "react-hot-loader": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}
